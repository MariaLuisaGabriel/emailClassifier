{
  "version": 2,
  "builds": [
        {
            "src": "./index.py",
            "use": "@vercel/python"
        },
        { "src": "index.html", "use": "@vercel/static" },
        { "src": "./classifier.py", "use": "@vercel/python" }
    ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.py" }
  ]
}